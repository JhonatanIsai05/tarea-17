#include<stdio.h>

using namespace std;

int main (int argc, char *[]){
	int var1=5;
	int var2=2;
	int resultado=0;
	
	//igual a ?
	resultado=var1 == var2;
	printf("%d==%d=%d\n",var1,var2, resultado);
	
	//menos igual que?
	resultado=var1 <= var2;
	printf("%d<=%d=%d\n",var1,var2, resultado);
	
	//mayor que?
	resultado=var1 > var2;
	printf("%d>%d=%d\n",var1,var2, resultado);
	
	//diferente de ?
	resultado=var1 != var2;
	printf("%d!=%d=%d\n",var1,var2, resultado);
	
	//uso de &&
	resultado = (var1 > var2) && (var1 != var2);
	printf("(%d > %d) && (%d != %d)=%d\n", var1, var2, resultado);
}
